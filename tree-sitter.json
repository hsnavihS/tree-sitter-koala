{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "koala",
      "camelcase": "koala",
      "title": "Koala",
      "scope": "source.koala",
      "file-types": [
        "kol"
      ],
      "injection-regex": "^koala$",
      "class-name": "TreeSitterKoala"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Tree sitter grammar for the Koala programming language",
    "authors": [
      {
        "name": "Shivansh Shukla",
        "email": "shuklashivansh26@gmail.com",
        "url": "https://www.linkedin.com/in/shuklashivansh/"
      }
    ],
    "links": {
      "repository": "https://github.com/hsnavihS/tree-sitter-koala"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}